<%- include('includes/head') %>
<%- include('includes/nav') %>
<div class="first_part">
  <div class="container">
    <div class="row">
      <div class="col-lg-2"></div>
      <div class="col-lg-8 my-3">
        <hr style="background-color: #FFF; margin: 1rem;">
        <h1 class="text-center">Coordenação</h1>
        <form action="/administracao/horarios" method="POST">
            <p class="text-center lead">Insira o RA do aluno para ver todos os dados</p>
            <input type="text" name="codigo_servidor" value="<%= codigo_servidor%>" hidden>
            <input type="text" name="senha" value="<%= senha%>" hidden>
            <input type="text" class="form-control" name="ra" autocomplete="off" placeholder="RA" >
            <input type="text" class="form-control" name="cpf" autocomplete="off" placeholder="CPF" onblur="formataCPF(this)">
            <input type="text" class="form-control" name="nome" autocomplete="off" placeholder="NOME COMPLETO" >
            <button class="btn button btn-success small">Ver</button>
          </form>
          <hr style="margin: 1rem;">
          <p class="text-center lead">Ocorrências Pendentes, Aprovadas ou Reprovadas</p>
          <form action="/administracao/advertencias" method="POST">
            <input type="text" name="codigo_servidor" value="<%= codigo_servidor%>" hidden>
            <input type="text" name="senha" value="<%= senha%>" hidden>
            <button class="btn button btn-info small">Ver</button>
          </form>
          <hr style="margin: 1rem;">
          <p class="text-center lead">Cadastros Gerais</p>
          <form action="/cadastro" method="POST">
            <input type="text" name="codigo_servidor" value="<%= codigo_servidor%>" hidden>
            <input type="text" name="senha" value="<%= senha%>" hidden>
            <button class="btn button btn-info small">Ver</button>
          </form>
          <hr style="background-color: #FFF; margin: 1rem;">
          <form action="/">
            <button class="btn button btn-dark">Voltar</button>
          </form>
        </div>
      </div>
      <div class="col-lg-2"></div>
    </div>
  </div>
</div>
<%- include('includes/footer');%>
