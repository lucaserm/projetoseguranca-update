<%- include('includes/head') %> <%- include('includes/nav') %>
<div class="container">
  <div class="row">
    <div class="col-lg-2"></div>
    <div class="col-lg-8 my-3">
      <h1 class="text-center">Estudantes</h1>
      <p class="text-center lead">Todos os alunos registrados:</p>
      <form action="/administracao/horarios" method="POST" enctype="multipart/form-data">
        <p class="text-center lead">Insira o RA do aluno para ver todos os dados</p>
        <input type="text" class="form-control" id="ra" name="ra" autocomplete="off"/>
        <input type="text" class="form-control" name="user" value="<%= user%>" hidden/>
        <input type="file" name="avatar" class="form-control">
        <button class="btn btn-success small" style="margin: 0">Ver</button>
      </form>
    </div>
    <div class="col-lg-2"></div>
  </div>
</div>

<%- include('includes/footer');%>
